<h1>Log In</h1>
<%= render 'layouts/errors' %>
<form  action="/session" method="post">
  <%= auth_token %>

  <label>Username
    <input type="text" name="user[username]">
  </label>

  <label>Password
    <input type="password" name="user[password]">
  </label>

  <input type="submit" value="Submit">
</form>
